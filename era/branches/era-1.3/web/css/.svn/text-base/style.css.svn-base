	padding : 0;
}

.edit_profile select {
	margin : 0;
	font-size : 0.9em;
}

.search_box input {
	float : left;
	padding : 0;
	margin : 6px 0 0 6px;
	border : 0;
	width : 163px;
	background : none;
	font-size : 0.9em;
}

.search_box input.search_submit {
	float : left;
	margin : 0;
	width : 79px;
	height : 27px;
	background-image : url("../images/searchbutton.png");
}

.search_box input.search_submit:hover {
	background-image : url("../images/searchbuttondown.png");
}

form.popup_form {
	margin-top : 1em;
}

.view_box label {
	font-weight : bold;
	display : block;
}

ul.adv_search li input,
ul.adv_search li select {
	margin : 0 0.3em;
	font-size : 0.9em;
}

ul.adv_search li input {
	width : 24.5em;
}
/*tables*/

table.display {
	width : 100%;
	/*margin-top: 1em;*/
}

table.display tr { /*border-bottom: 1px dotted;*/
}

table.display tr.first {
	border-bottom : none;
}

table.display th {
	text-align : left;
}

table.display td {
	padding : 0.5em 0 0.5em 1em;
}

td.toolbox a.add_button {
	display : none;
}

a.cancel_link {
	margin-left : 1em;
	font-weight : bold;
	cursor : pointer;
}
/* Auto Complete */

div.autocomplete {
	position : absolute;
	width : 250px;
	background-color : white;
	border : 1px solid #888;
	margin : 0;
	padding : 0;
}

div.autocomplete ul {
	list-style-type : none;
	margin : 0;
	padding : 0;
}

div.autocomplete ul li.selected {
	background-color : #ffb;
}

div.autocomplete ul li {
	list-style-type : none;
	display : block;
	margin : 0;
	padding : 2px;
	height : 1.1em;
	cursor : pointer;
}

.removed {
	text-decoration : line-through;
}

.approve_buttons {
	margin-top : 1em;
	text-align : center;
	float : left;
	font-size : 1em;
}

.approve_buttons input.save_button {
	margin-left : 0.2em;
	height : 2em;
	width : 5em;
}

div.modal_box {
	text-align : center;
}

div.modal_box div.message {
	margin : 1em 1em 1em 1em;
	line-height : 1.2em;
	color : #454545;
}

div.modal_box input.save_button {
	margin-left : 0em;
	height : 2em;
	width : 3em;
	text-align : center;
}

a.handle {
	white-space : nowrap;
}

select.reject {
	margin-top : 0.5em;
	font-family : verdana, arial, helvetica, sans-serif;
}

input.e-thesis {
	margin-left : 6em;
	height : 3em;
	width : 20em;
	white-space : normal;
	text-align : center;
	vertical-align : middle;
}

div.default_subheader {
	width : 14em;
	display : inline;
	height : 2.5em;
	white-space : normal !important;
	text-align : left;
	float : left;
}

label.no_result {
	background-color : #fcc;
	padding : 0.1em 0.3em 0.1em 0.3em;
}

div.thesis_message {
	color : #FF0000;
	font-weight : bold;
	margin : 1em 1em 1em 1em;
}

h1.noscript {
	color : #ffffff;
	margin : 0.5em 2em 0 2em;
}

h1.noscript a {
	font-family : georgia, "times new roman", times, serif;
	font-size : 100%;
	color : #00FF00;
}

h1.noscript a:visited {
	font-family : georgia, "times new roman", times, serif;
	font-size : 100%;
	color : #00FF00;
}

