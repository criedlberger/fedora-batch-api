<input-forms>

	<!-- The form-map maps collection handles to forms. DSpace does not -->
	<!-- require that a collection's name be unique, even within a community . -->
	<!-- DSpace does however insure that each collection's handle is unique. -->
	<!-- Form-map provides the means to associate a unique collection name -->
	<!-- with a form. The form-map also provides the special handle "default" -->
	<!-- (which is never a collection), here mapped to "traditional". Any -->
	<!-- collection which does not appear in this map will be associated with -->
	<!-- the mapping for handle "default". -->
	<form-map>

		<!-- default: default form -->
		<name-map id="default" item-type="default" form-name="default" />

		<!-- thesis: Theses and Dissertations collection form -->
		<name-map id="thesis" item-type="thesis" form-name="thesis" />

		<!-- ser: Structure Engineering Report collection form -->
		<name-map id="ser" item-type="ser" form-name="ser" />
		
		<!-- tr: Structure Engineering Report collection form -->
		<name-map id="tr" item-type="tr" form-name="tr" />
		
	</form-map>

	<!-- The form-definitions map lays out the detailed definition of all the -->
	<!-- submission forms.Each separate form set has a unique name as an -->
	<!-- attribute. This name matches one of the names in the form-map. One -->
	<!-- named form set has the name "traditional"; as this name suggests, -->
	<!-- it is the old style and is also the default, which gets used when -->
	<!-- the specified collection has no correspondingly named form set. -->
	<!-- -->
	<!-- Each form set contains an ordered set of pages; each page defines -->
	<!-- one submission metadata entry screen. Each page has an ordered list -->
	<!-- of field definitions, Each field definition corresponds to one -->
	<!-- metatdata entry (a so-called row), which has a DC element name, a -->
	<!-- displayed label, a text string prompt which is called a hint , and -->
	<!-- an input-type. Each field also may hold optional elements: DC -->
	<!-- qualifier name, a repeatable flag, and a text string whose presence -->
	<!-- serves as a 'this field is required' flag. -->
	<!-- -->
	<!-- Internationalization supported for label and hint: -->
	<!-- The application will look up a message in resources.properties file -->
	<!-- using field id. -->
	<!-- -->
	<!-- *Note: dc-element + dc-qualifier + attribute@name + attribute@value -->
	<!-- have to be unique value. The application will use as a key -->
	<!-- to find data and display on input form correctly -->

	<!-- The id attribute(option) in label, hint and displayed-value tag will -->
	<!-- look up a label in resources.propertiies instread of the tag value. -->
	<!-- Ex: <label id="dc-type" /> -->
	<!-- <hint id="dc-type.hint" /> -->
	<form-definitions>

		<!-- default form -->
		<form name="default" enabled="true">
			<!-- identifier from dublin core -->
			<field>
				<dc-element>dc:identifier</dc-element>
				<input-type>hidden</input-type>
				<label>Identifier</label>
			</field>
			<!-- auto-generated fields -->
			<field>
				<dc-element>dc:format</dc-element>
				<attribute name="xsi:type" value="dcterms:IMT" />
				<label>Format</label>
				<input-type>hidden</input-type>
			</field>
			<field>
				<dc-element>dcterms:extent</dc-element>
				<label>Extent</label>
				<input-type>hidden</input-type>
			</field>
			<!-- input fields -->
			<field>
				<dc-element>dc:type</dc-element>
				<repeatable>false</repeatable>
				<label>Type of Item</label>
				<input-type value-pairs-name="default-types">dropdown</input-type>
				<validate>
					<required>true</required>
				</validate>
			</field>
			<field>
				<dc-element>dc:language</dc-element>
				<attribute name="xsi:type" value="dcterms:ISO639-3" />
				<label>Language</label>
				<input-type value-pairs-name="default-languages">dropdown</input-type>
				<validate>
					<required>true</required>
				</validate>
				<value>eng</value>
			</field>
			<field>
				<dc-element>dc:title</dc-element>
				<repeatable>false</repeatable>
				<label>Title or Caption</label>
				<input-type>text</input-type>
				<style>width: 40em;</style>
				<validate>
					<required>true</required>
				</validate>
			</field>
			<field>
				<dc-element>dc:creator</dc-element>
				<repeatable>true</repeatable>
				<label>Author or Creator</label>
				<input-type>text</input-type>
				<hint>example: Doe, Jane B.</hint>
				<validate>
					<required>true</required>
				</validate>
			</field>
			<field>
				<dc-element>dc:contributor</dc-element>
				<repeatable>true</repeatable>
				<label>Additional Contributors</label>
				<input-type>text</input-type>
				<hint>examples: Doe, Jane B. || Smith Brothers Publishing</hint>
			</field>
			<field>
				<dc-element>dc:subject</dc-element>
				<repeatable>true</repeatable>
				<display>3</display>
				<label>Subject/Keyword</label>
				<input-type>text</input-type>
				<hint>Enter subjects or keywords that describe the item. These are the keywords that will be used when someone
					searches for your item. Enter only one word or phrase per box.</hint>
				<validate>
					<required>true</required>
				</validate>
			</field>
			<field>
				<input-type>label</input-type>
				<label>Is this item about:</label>
			</field>
			<field>
				<dc-element>dcterms:temporal</dc-element>
				<label>a time?</label>
				<indent>true</indent>
				<input-type>text</input-type>
				<hint>examples: 2009/09/01 || 2003/2010 (a date range) || the Enlightenment</hint>
			</field>
			<field>
				<dc-element>dcterms:spatial</dc-element>
				<label>a place?</label>
				<indent>true</indent>
				<input-type>text</input-type>
				<hint>example: Canada, Alberta, Edmonton</hint>
			</field>
			<field>
				<dc-element>dc:description</dc-element>
				<label>Description</label>
				<input-type>textarea</input-type>
				<hint>If your item has an abstract put it here.</hint>
				<!-- <validate> -->
				<!-- <maxlength>3072</maxlength> -->
				<!-- </validate> -->
			</field>
			<field>
				<dc-element>dcterms:isversionof</dc-element>
				<label>Citation for previous publication</label>
				<input-type>textarea</input-type>
				<hint>If your item has been previously published, provide a citation.</hint>
			</field>
			<field>
				<dc-element>dcterms:created</dc-element>
				<label>Date Created</label>
				<input-type>text</input-type>
				<hint>example: 2009/09/01</hint>
			</field>
			<field>
				<dc-element>dc:source</dc-element>
				<label>Source</label>
				<input-type>text</input-type>
				<hint>Brief information about a physical item from which this item was derived.</hint>
				<validate>
					<maxlength>1024</maxlength>
				</validate>
			</field>
			<field>
				<dc-element>dc:relation</dc-element>
				<label>Link to related item</label>
				<input-type>text</input-type>
				<hint>must be a url - example: http://www.ualberta.ca</hint>
				<validate>
					<url>true</url>
				</validate>
			</field>
			<field>
				<dc-element>uploadfile</dc-element>
				<input-type>file</input-type>
				<repeatable>true</repeatable>
			</field>
		</form>

		<!-- thesis form -->
		<form name="thesis" enabled="true">
			<!-- message in form message area -->
			<field>
				<dc-element>form-enabled-message</dc-element>
				<input-type>message</input-type>
				<value>
					&lt;div class=&quot;thesis_message&quot;&gt;
					Thesis submission deadline for Spring 2013 Convocation without Winter Term 2013 registration is January 8, 2013 at 4 pm.
					&lt;br/&gt;
					Thesis submission deadline for Spring 2013 Convocation with THES 910 registration in Winter Term 2013 is January 31, 2013 at 4 pm 
					(only certain non-instructional fees are assessed).  FGSR will register you in THES 910 when your thesis has been successfully accepted.
					&lt;br/&gt;&lt;br/&gt;
					Please contact &lt;a href=&quot;mailto:grad.services@ualberta.ca?subject=Questions regarding thesis submission&quot;&gt;grad.services@ualberta.ca&lt;/a&gt; 
					with questions regarding thesis submission. 
					&lt;/div&gt;
				</value>
			</field>
			<!-- submit deadline message -->
			<field>
				<dc-element>form-disabled-message</dc-element>
				<input-type>message</input-type>
				<value>
					&lt;div class=&quot;thesis_message&quot;&gt;
					Thesis submission deadline for Spring 2013 Convocation without Winter Term 2013 registration is January 8, 2013 at 4 pm.
					&lt;br/&gt;
					Thesis submission deadline for Spring 2013 Convocation with THES 910 registration in Winter Term 2013 is January 31, 2013 at 4 pm 
					(only certain non-instructional fees are assessed).  FGSR will register you in THES 910 when your thesis has been successfully accepted.
					&lt;br/&gt;&lt;br/&gt;
					Please contact &lt;a href=&quot;mailto:grad.services@ualberta.ca?subject=Questions regarding thesis submission&quot;&gt;grad.services@ualberta.ca&lt;/a&gt; 
					with questions regarding thesis submission. 
					&lt;br/&gt;&lt;br/&gt;
					Submission and re-submission is currently suspended until 9:00 a.m., October 2, 2012.
					&lt;/div&gt;
				</value>
			</field>
			<field>
				<dc-element>form-enabled-footer</dc-element>
				<input-type>message</input-type>
				<value>
					&lt;p class=&quot;regular&quot;&gt;If you are not ready to submit, please click on &quot;&lt;strong&gt;Save, I'll finish later
					&lt;/strong&gt;&quot; button to save this form. The form will be saved to your account and you can log in later and complete your 
					submission.&lt;/p&gt;
				</value>
			</field>
			<field>
				<dc-element>form-disabled-footer</dc-element>
				<input-type>message</input-type>
				<value>
					&lt;div class=&quot;thesis_message&quot;&gt;
					You will be able to submit again at 9:00 a.m. on October 2, 2012.. For now, save your work, so that you can return and submit when you are again permitted to proceed.
					&lt;/div&gt;
				</value>
			</field>
			<!-- identifier from dublin core -->
			<field>
				<dc-element>dc:identifier</dc-element>
				<input-type>hidden</input-type>
				<label>Identifier</label>
			</field>
			<!-- auto-generated fields -->
			<field>
				<dc-element>dc:format</dc-element>
				<attribute name="xsi:type" value="dcterms:IMT" />
				<label>Format</label>
				<input-type>hidden</input-type>
			</field>
			<field>
				<dc-element>dcterms:extent</dc-element>
				<label>Extent</label>
				<input-type>hidden</input-type>
			</field>
			<!-- item type hidden field -->
			<field>
				<dc-element>dc:type</dc-element>
				<label>Type of Item</label>
				<input-type>hidden</input-type>
				<validate>
					<required>true</required>
				</validate>
				<value>Thesis</value>
			</field>
			<!-- input fields -->
			<field>
				<dc-element>dc:creator</dc-element>
				<repeatable>false</repeatable>
				<label>Author</label>
				<input-type>text</input-type>
				<hint>Enter your last name, followed by a comma, then your first name and middle initial(s) using upper and lower
					case. &lt;br /&gt;Example: Student, John Jr </hint>
				<validate>
					<required>true</required>
					<regexp>,</regexp>
				</validate>
			</field>
			<field>
				<dc-element>dc:title</dc-element>
				<repeatable>false</repeatable>
				<label>Title</label>
				<input-type>text</input-type>
				<style>width: 40em;</style>
				<hint> Enter the title of your thesis as it appears on your thesis title page. Do not put a period at the end of the
					title. &lt;br /&gt;Example: Blue: How Alberta’s Tories Have Shaped Western Canadian Politics</hint>
				<validate>
					<required>true</required>
					<maxlength>1024</maxlength>
				</validate>
			</field>
			<field>
				<dc-element>dcterms:alternative</dc-element>
				<repeatable>true</repeatable>
				<label>Other Title(s)</label>
				<input-type>text</input-type>
				<hint>Enter the secondary/translated title of your thesis, if appropriate.</hint>
				<validate>
					<required>false</required>
					<maxlength>1024</maxlength>
				</validate>
			</field>
			<field>
				<dc-element>dc:language</dc-element>
				<attribute name="xsi:type" value="dcterms:ISO639-3" />
				<repeatable>false</repeatable>
				<label>Language</label>
				<input-type value-pairs-name="default-languages">dropdown</input-type>
				<hint>Select the language in which your thesis is written.</hint>
				<validate>
					<required>true</required>
				</validate>
			</field>
			<field>
				<dc-element>dc:subject</dc-element>
				<repeatable>true</repeatable>
				<display>3</display>
				<label>Subject Keyword(s)</label>
				<input-type>text</input-type>
				<hint>Enter keywords from your thesis title, chapter headings, etc. These are the keywords that will be used when
					someone searches your thesis. Enter only one word or phrase per box.</hint>
				<validate>
					<required>true</required>
				</validate>
			</field>
			<field>
				<dc-element>dcterms:abstract</dc-element>
				<repeatable>false</repeatable>
				<label>Abstract</label>
				<input-type>textarea</input-type>
				<hint>Enter the abstract of your thesis (up to 150 words for a master’s thesis, up to 350 for a doctoral thesis).
				</hint>
				<validate>
					<required>true</required>
					<!-- <maxlength>3072</maxlength> -->
				</validate>
			</field>
			<field>
				<dc-element>dcterms:isversionof</dc-element>
				<repeatable>true</repeatable>
				<label>Citation for Previous Publication</label>
				<input-type>textarea</input-type>
				<hint>Enter a citation for any instance in which a portion of your thesis has been previously published or publicly
					distributed. Enter only one citation per box.</hint>
				<validate>
					<required>false</required>
					<maxlength>3072</maxlength>
				</validate>
			</field>
			<field>
				<dc-element>thesis:degree</dc-element>
				<dc-qualifier>thesis:level</dc-qualifier>
				<repeatable>false</repeatable>
				<label>Degree Level</label>
				<input-type value-pairs-name="thesis-degree-levels">dropdown</input-type>
				<validate>
					<required>true</required>
				</validate>
			</field>
			<field>
				<dc-element>thesis:degree</dc-element>
				<dc-qualifier>thesis:name</dc-qualifier>
				<repeatable>false</repeatable>
				<label>Degree</label>
				<input-type value-pairs-name="thesis-degree-names">dropdown</input-type>
				<hint>Choose your degree name</hint>
				<validate>
					<required>true</required>
				</validate>
			</field>
			<field>
				<input-type>label</input-type>
				<label>Department</label>
			</field>
			<field>
				<input-type>label</input-type>
				<label>
					&lt;div style="font-weight: normal;"&gt;
					If your degree is interdisciplinary, first choose your main
					department, and then add the conjoint department.
					&lt;br/&gt;&lt;br/&gt;
					All other students, enter Department by
					choosing from Main Department.
					&lt;/div&gt; </label>
			</field>
			<field>
				<dc-element>thesis:degree</dc-element>
				<dc-qualifier>thesis:discipline</dc-qualifier>
				<repeatable>true</repeatable>
				<label>Main Department</label>
				<input-type value-pairs-name="thesis-departments">dropdown</input-type>
				<hint>Choose the name of your main department/academic unit exactly as it appears on your thesis title page.</hint>
				<validate>
					<required>true</required>
				</validate>
			</field>
			<!-- grantor hidden field -->
			<field>
				<dc-element>thesis:degree</dc-element>
				<dc-qualifier>thesis:grantor</dc-qualifier>
				<label>Grantor</label>
				<input-type>hidden</input-type>
				<validate>
					<required>true</required>
				</validate>
				<value>University of Alberta</value>
			</field>
			<field>
				<dc-element>eraterms:specialization</dc-element>
				<repeatable>false</repeatable>
				<label>Specialization</label>
				<input-type>text</input-type>
				<hint>Enter your specialization (if appropriate)</hint>
				<validate>
					<required>false</required>
				</validate>
			</field>
			<field>
				<dc-element>eraterms:graduationdate</dc-element>
				<repeatable>false</repeatable>
				<label>Graduation Date</label>
				<input-type value-pairs-name="thesis-graduation-dates">daterange_value</input-type>
				<validate>
					<required>true</required>
				</validate>
			</field>
			<field>
				<dc-element>thesis:contributor</dc-element>
				<attribute name="role" value="advisor" />
				<repeatable>true</repeatable>
				<label>Supervisor/Co-supervisor and their Department(s)</label>
				<input-type>text</input-type>
				<style>width: 40em;</style>
				<hint>Enter the name and department of your supervisor. Add additional boxes for co-supervisors as necessary.
					&lt;br/&gt;e.g. Smith, John (Computing Science)</hint>
				<validate>
					<required>true</required>
					<!-- <regexp>[,()]</regexp> -->
				</validate>
			</field>
			<field>
				<dc-element>thesis:contributor</dc-element>
				<attribute name="role" value="committeemember" />
				<repeatable>true</repeatable>
				<display>5</display>
				<label>Examining Committee Members and their Departments</label>
				<input-type>text</input-type>
				<style>width: 40em;</style>
				<hint>Enter the name and department of your examining committee members. Add boxes for all members. &lt;br/&gt;e.g.
					Smith, John (Computing Science)</hint>
				<validate>
					<required>true</required>
					<!-- <regexp>[,()]</regexp> -->
				</validate>
			</field>
			<field>
				<dc-element>uploadfile</dc-element>
				<input-type>file</input-type>
				<repeatable>false</repeatable>
			</field>
		</form>

