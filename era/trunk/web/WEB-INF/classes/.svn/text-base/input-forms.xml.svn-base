			<field>
				<dc-element>dc:relation</dc-element>
				<label>Link to related item</label>
				<input-type>text</input-type>
				<hint>must be a url - example: http://www.ualberta.ca</hint>
				<validate>
					<url>true</url>
				</validate>
			</field>
			<field>
				<dc-element>uploadfile</dc-element>
				<input-type>file</input-type>
				<repeatable>true</repeatable>
			</field>
		</form>

		<!-- Technical Reports form -->
		<form name="tr" enabled="true">
			<!-- auto-generated fields -->
			<field>
				<dc-element>dc:format</dc-element>
				<attribute name="xsi:type" value="dcterms:IMT" />
				<label>Format</label>
				<input-type>hidden</input-type>
			</field>
			<field>
				<dc-element>dcterms:extent</dc-element>
				<label>Extent</label>
				<input-type>hidden</input-type>
			</field>
			<!-- input fields -->
			<field>
				<dc-element>dc:date</dc-element>
				<input-type>text</input-type>
				<label>Year</label>
				<hint>Enter a Year: 2008</hint>
				<validate>
					<required>true</required>
					<regexp>^[12][0-9]{3}$</regexp>
				</validate>
			</field>
			<field>
				<dc-element>dcterms:identifier</dc-element>
				<attribute name="xsi:type" value="eraterms:trid" />
				<input-type>text</input-type>
				<label>Technical Report ID</label>
				<hint>Enter a Technical Report ID. example: TR08-04</hint>
				<validate>
					<required>true</required>
					<regexp>^TR[0-9][0-9]-[0-9][0-9]$</regexp>
				</validate>
			</field>
			<field>
				<dc-element>dc:title</dc-element>
				<repeatable>false</repeatable>
				<label>Title or Caption</label>
				<input-type>text</input-type>
				<style>width: 40em;</style>
				<validate>
					<required>true</required>
				</validate>
			</field>
			<field>
				<dc-element>dc:creator</dc-element>
				<repeatable>true</repeatable>
				<label>Author or Creator</label>
				<input-type>text</input-type>
				<hint>example: Doe, Jane B.</hint>
				<validate>
					<required>true</required>
				</validate>
			</field>
			<field>
				<dc-element>dc:description</dc-element>
				<repeatable>true</repeatable>
				<label>Description</label>
				<input-type>textarea</input-type>
				<hint>If your item has an abstract put it here.</hint>
			</field>
			<field>
				<dc-element>dc:subject</dc-element>
				<repeatable>true</repeatable>
				<label>Subject/Keyword</label>
				<input-type>text</input-type>
				<hint>Enter keywords from your thesis title, chapter headings, etc. These are the keywords that will be used when
					someone searches your thesis. Enter only one word or phrase per box.</hint>
				<validate>
					<required>true</required>
				</validate>
			</field>
			<!-- item type hidden field -->
			<field>
				<dc-element>dc:type</dc-element>
				<label>Type of Item</label>
				<input-type>hidden</input-type>
				<validate>
					<required>true</required>
				</validate>
				<value>Report</value>
			</field>
			<field>
				<dc-element>dc:language</dc-element>
				<attribute name="xsi:type" value="dcterms:ISO639-3" />
				<label>Language</label>
				<input-type value-pairs-name="default-languages">dropdown</input-type>
				<validate>
					<required>true</required>
				</validate>
				<value>eng</value>
			</field>
			<field>
				<dc-element>dc:rights</dc-element>
				<repeatable>false</repeatable>
				<label>Rights</label>
				<input-type>text</input-type>
				<style>width: 40em;</style>
			</field>
			<field>
				<dc-element>uploadfile</dc-element>
				<input-type>file</input-type>
				<repeatable>true</repeatable>
			</field>
		</form>
	</form-definitions>

